#include <iostream>
#include <bits/stdc++.h>

using namespace std;

vector <int> sum(vector <int> a,vector <int> b)
{
    vector <int> c(1005,0);

    int r=0;
    for(int i = 0 ; i < 1005 ; i++)
    {
        c[i]=a[i]+b[i]+r;
        r=c[i]/10;
        c[i]%=10;
    }
    return c;
}

void print(vector <int> a)
{
    int i = 1000; while(a[i]==0) i--;

    for( ; i >= 0 ; i--) cout << a[i];
    cout << "\n";
}

int main()
{
    int n;
    while(cin >> n)
    {
        vector <int> a(1005,0); a[0]=1;
        vector <int> b(1005,0); b[0]=1;

        if(n<=2) print(a);
        else
        {
            vector <int> c(1005,0);

            n-=2;
            while(n--)
            {
                c=sum(a,b);
                a=b;
                b=c;
            }
            print(c);
        }
    }
    return 0;
}
